// Prisma schema for PettyGo ERP users

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int     @id @default(autoincrement())
  username String  @unique
  password String
  role     String
  createdAt DateTime @default(now())
}
